{
  "name": "jspdf-utils",
  "version": "0.1.0",
  "description": "Utility helpers for jsPDF's doc.html() renderer",
  "type": "module",
  "main": "dist/html-to-pdf.js",
  "types": "dist/html-to-pdf.d.ts",
  "exports": {
    ".": {
      "types": "./dist/html-to-pdf.d.ts",
      "import": "./dist/html-to-pdf.js"
    }
  },
  "peerDependencies": {
    "html2canvas": "^1.4.1",
    "jspdf": "^4.1.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --emitDeclarationOnly",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "html2canvas": "^1.4.1",
    "jspdf": "^4.1.0",
    "typescript": "^5.7.0",
    "vite": "^6.0.0"
  }
}
